================================================================================
<===> interleaved/input.scss
@use "plain";

<===> interleaved/plain.css
a {
  @media b {
    c {
      @media (d) {
        e: f;
      }
    }
  }
}

<===> interleaved/output.css
@media b {
  a {
    c {
      @media (d) {
        e: f;
      }
    }
  }
}
