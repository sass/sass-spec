<===> in_style_rule/in_bubbled_rule/input.scss
@use 'plain';

<===> in_style_rule/in_bubbled_rule/plain.css
a {
  b {c: d}
  @e {@nest {f: g}}
}

<===> in_style_rule/in_bubbled_rule/output.css
a {
  b {
    c: d;
  }
}
@e {
  a {
    f: g;
  }
}

