<===> in_style_rule/before_nested_rule/input.scss
@use 'plain';

<===> in_style_rule/before_nested_rule/plain.css
a {
  @nest {b: c}
  d {e: f}
}

<===> in_style_rule/before_nested_rule/output.css
a {
  b: c;
  d {
    e: f;
  }
}

