<===> isolated/input.scss
@use "sass:selector";
a {b: selector.unify(".c > .d + .e", ".f .g ~ .h")}

<===> isolated/output.css
a {
  b: .f .c > .g ~ .d + .e.h, .f .c > .d.g + .e.h;
}

