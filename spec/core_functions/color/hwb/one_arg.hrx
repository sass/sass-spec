<===> alpha/in_gamut/transparent/input.scss
@use 'sass:color';
a {b: color.hwb(180 30% 40% / 0)}

<===> alpha/in_gamut/transparent/output.css
a {
  b: hsla(180deg, 33.3333333333%, 45%, 0);
}

