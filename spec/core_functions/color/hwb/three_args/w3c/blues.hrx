<===> input.scss
@use '../test-hue' as *;
@include test-hue(240);

<===> output.css
whiteness-0 {
  blackness-0: blue;
  blackness-20: #0000cc;
  blackness-40: #000099;
  blackness-60: #000066;
  blackness-80: #000033;
  blackness-100: black;
}

whiteness-20 {
  blackness-0: #3333ff;
  blackness-20: #3333cc;
  blackness-40: #333399;
  blackness-60: #333366;
  blackness-80: #333333;
  blackness-100: hsl(0deg, 0%, 16.6666666667%);
}

whiteness-40 {
  blackness-0: #6666ff;
  blackness-20: #6666cc;
  blackness-40: #666699;
  blackness-60: #666666;
  blackness-80: #555555;
  blackness-100: hsl(0deg, 0%, 28.5714285714%);
}

whiteness-60 {
  blackness-0: #9999ff;
  blackness-20: #9999cc;
  blackness-40: #999999;
  blackness-60: hsl(0deg, 0%, 50%);
  blackness-80: hsl(0deg, 0%, 42.8571428571%);
  blackness-100: hsl(0deg, 0%, 37.5%);
}

whiteness-80 {
  blackness-0: #ccccff;
  blackness-20: #cccccc;
  blackness-40: #aaaaaa;
  blackness-60: hsl(0deg, 0%, 57.1428571429%);
  blackness-80: hsl(0deg, 0%, 50%);
  blackness-100: hsl(0deg, 0%, 44.4444444444%);
}

whiteness-100 {
  blackness-0: white;
  blackness-20: hsl(0deg, 0%, 83.3333333333%);
  blackness-40: hsl(0deg, 0%, 71.4285714286%);
  blackness-60: hsl(0deg, 0%, 62.5%);
  blackness-80: hsl(0deg, 0%, 55.5555555556%);
  blackness-100: hsl(0deg, 0%, 50%);
}
