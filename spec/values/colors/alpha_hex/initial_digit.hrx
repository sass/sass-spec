<===> input.scss
@use "sass:color";
a {
  four-digit: #0123;
  eight-digit: #98765432;

  // Verify that the color channels are set correctly.
  four-digit-red: color.channel(#0123, "red");
  four-digit-green: color.channel(#0123, "green");
  four-digit-blue: color.channel(#0123, "blue");
  four-digit-alpha: color.channel(#0123, "alpha");

  eight-digit-red: color.channel(#98765432, "red");
  eight-digit-green: color.channel(#98765432, "green");
  eight-digit-blue: color.channel(#98765432, "blue");
  eight-digit-alpha: color.channel(#98765432, "alpha");
}

<===> output.css
a {
  four-digit: rgba(0, 17, 34, 0.2);
  eight-digit: rgba(152, 118, 84, 0.1960784314);
  four-digit-red: 0;
  four-digit-green: 17;
  four-digit-blue: 34;
  four-digit-alpha: 0.2;
  eight-digit-red: 152;
  eight-digit-green: 118;
  eight-digit-blue: 84;
  eight-digit-alpha: 0.1960784314;
}
