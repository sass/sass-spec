<===> input.scss
@use "sass:list";
@use "sass:meta";
@function foo() {
  @return 1+2 3/4 5+6;
}

@mixin bar($x: 3/4) {
  bar-content: $x;
}

div {
  content: foobar(1+2 3/4 5+6, orange);
  content: list.append(1+2 2/3 5+6, orange);
  content: 1+2 2/3 5+6;
  content: meta.type-of(2/3);
  content: meta.type-of(orange);
  content: foo();
  @include bar();
}
<===> output.css
div {
  content: foobar(3 3 / 4 11, orange);
  content: 3 2 / 3 11 / orange;
  content: 3 2 / 3 11;
  content: list;
  content: color;
  content: 3 3 / 4 11;
  bar-content: 3 / 4;
}
