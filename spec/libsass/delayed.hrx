<===> input.scss
@use "sass:list";
$x: a 3/4 b;
$y: hey;

@function foo() {
  @return 3/4;
}

div {
  hoo: 3/4;
  goo: list.nth($x, 2);
  foo: 15 / list.nth($x, 2);
  foo: .25 + list.nth($x, 2);
  coo: 2/3 / list.nth($x, 2);
  bar: $y and true;
  bar: false and true;
  bar: (false) and true;
  @each $elem in $x {
    blah: $elem;
  }
  bloo: foo();
  @warn 2/3;
  blix: "hey #{list.nth($x, 2)} ho";
}

@media screen and (hux: 3/4) {
  div {
    color: red;
  }
}

@warn "blah blah";

div {
  blah: "ho #{list.nth($x, 2) } ho";
}

span {
  fludge: (true and 3/4);
}
