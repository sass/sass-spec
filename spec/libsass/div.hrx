<===> input.scss
@use "sass:list";
@use "sass:meta";
$x: 3/4;
$xs: hey 3/4 ho;

div {
  /* $x: 3/4 */
  a: $x;
  b: hey $x ho;
  /* $xs: hey 3/4 ho */
  c: $xs;
  d: list.nth($xs, 2);
  e: list.nth($xs, 2) == 0.75;
  f: meta.type-of(list.nth($xs, 2));
}
<===> output.css
div {
  /* $x: 3/4 */
  a: 3 / 4;
  b: hey 3 / 4 ho;
  /* $xs: hey 3/4 ho */
  c: hey 3 / 4 ho;
  d: 4 ho;
  e: false;
  f: list;
}
