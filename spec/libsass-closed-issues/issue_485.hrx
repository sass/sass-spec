<===> options.yml
:ignore_for:
  - libsass

<===> input.scss
@media not all and (monochrome) { a {foo: bar} }
@media not screen and (color), print and (color) { a {foo: bar} }
@media (not (screen and (color))), print and (color) { a {foo: bar} }

<===> output.css
@media not all and (monochrome) {
  a {
    foo: bar;
  }
}
@media not screen and (color), print and (color) {
  a {
    foo: bar;
  }
}
@media not (color), print and (color) {
  a {
    foo: bar;
  }
}
